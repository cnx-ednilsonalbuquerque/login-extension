{
  "manifest_version": 3,
  "name": "My Logins",
  "version": "1.0",
  "description": "Save and use logins in form pages",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html",
    "default_title": "MyLogins"
  },
  "icons": {
    "16": "favicon-16x16.png",
    "48": "favicon-32x32.png",
    "128": "favicon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ]
}
